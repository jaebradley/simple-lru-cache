{
  "name": "@jaebradley/simple-lru-cache",
  "version": "0.0.0-development",
  "description": "A Simple LRU Cache",
  "keywords": [
    "lru",
    "cache",
    "lru cache"
  ],
  "homepage": "https://github.com/jaebradley/simple-lru-cache#readme",
  "bugs": {
    "url": "https://github.com/jaebradley/simple-lru-cache/issues"
  },
  "license": "MIT",
  "author": {
    "name": "jae.b.bradley@gmail.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build"
  ],
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaebradley/simple-lru-cache.git"
  },
  "scripts": {
    "build": "rollup -c",
    "build:prod": "BABEL_ENV=production npm run build",
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "deploy": "npm run travis-deploy-once 'npm run semantic-release'",
    "gc": "commit",
    "is-es5": "es-check es5 build/index.js",
    "lint": "eslint --ext .js .",
    "prepare": "npm run build:prod",
    "semantic-release": "semantic-release",
    "test": "BABEL_ENV=test jest src/ --coverage --no-cache",
    "travis-deploy-once": "travis-deploy-once"
  },
  "browser": "build/index.js",
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-angular": "^7.0.1",
    "@commitlint/prompt": "^7.0.0",
    "@commitlint/prompt-cli": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "23.0.1",
    "benchmark": "^2.1.4",
    "codecov": "^3.0.2",
    "es-check": "^2.0.4",
    "eslint": "^5.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lru_map": "^0.3.3",
    "quick-lru": "^1.1.0",
    "rollup": "^0.64.0",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-babel-minify": "^7.0.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-filesize": "^4.0.1",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^2.0.0",
    "semantic-release": "^15.1.7",
    "travis-deploy-once": "^5.0.0"
  },
  "readme": "https://github.com/jaebradley/simple-lru-cache#readme"
}
